      owl:onProperty :weeks ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:decimal ;
      owl:onProperty :years ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:hasValue <http://www.opengis.net/def/uom/ISO-8601/0/Gregorian> ;
      owl:onProperty :hasTRS ;
    ] ;
